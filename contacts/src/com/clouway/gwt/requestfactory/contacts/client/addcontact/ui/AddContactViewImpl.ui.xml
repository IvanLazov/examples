             xmlns:d='urn:import:com.clouway.gwt.requestfactory.contacts.client.addcontact.editor'>

  <g:HTMLPanel>

    <div style="margin-bottom: 40px">
      <g:Button ui:field="view">View All</g:Button>
    </div>

    <div>
      <d:AddContactEditor ui:field="editor"/>
